{
    "tags": {
        "name": {
            "min_length": 2, 
            "max_length": 40,
            "regex": "/[^A-Za-z0-9 ]/"
        }
    },
    "riders": {
        "firstname": {
            "min_length": 2, 
            "max_length": 40
        },
        "lastname": {
            "min_length": 2, 
            "max_length": 40
        },
        "level": {
            "values": [
                {"code": "beginer", "name": "beginer"},
                {"code": "confirm", "name": "confirm"},
                {"code": "pro", "name": "pro"}
            ]
        }
    },
    "videos": {
        "provider_id": {
            "values": ["youtube"]
        }
    }
}
